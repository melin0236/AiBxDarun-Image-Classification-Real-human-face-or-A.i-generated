import streamlit as st
from PIL import Image
import os
import json
import numpy as np
import pandas as pd
from fastai.vision.all import load_learner
from fastai.vision.all import PILImage
from fastai.vision.all import Resize
from fastai.vision.all import *
from fastai.vision.data import ImageDataLoaders
import pathlib
import urllib

st.set_page_config(page_title="RealORFake",page_icon="ü§ñ",layout="wide",initial_sidebar_state="expanded")

plt = platform.system()
if plt == 'Windows': pathlib.PosixPath = pathlib.WindowsPath
if plt == 'Linux': pathlib.WindowsPath = pathlib.PosixPath

model = load_learner('Cloud_resnet50_fastai.pkl',cpu=True) # load model

st.title("**Cloud Classification (Cloud Classy) ‡∏°‡∏≤‡∏°‡∏∞‡∏°‡∏≤‡πÅ‡∏¢‡∏Å‡πÄ‡∏°‡∏Ü‡∏Å‡∏±‡∏ô**") #Title
st.subheader('"Cloud_Classy" ‡∏Ñ‡∏∑‡∏≠‡πÇ‡∏õ‡∏£‡πÄ‡∏à‡∏Ñ‡∏ó‡∏µ‡πà‡πÉ‡∏ä‡πâ‡∏Ñ‡∏±‡∏î‡πÅ‡∏¢‡∏Å‡∏õ‡∏£‡∏∞‡πÄ‡∏†‡∏ó‡πÅ‡∏•‡∏∞‡∏ö‡∏≠‡∏Å‡∏•‡∏±‡∏Å‡∏©‡∏ì‡∏∞‡∏Å‡∏≤‡∏£‡πÄ‡∏Å‡∏¥‡∏î.') #information
st.markdown("Please upload your image of cloud or use the sample images on the left sidebar.") #information
st.sidebar.image('./logo.png')
st.sidebar.markdown("**‡∏ñ‡πâ‡∏≤‡∏Ç‡∏µ‡πâ‡πÄ‡∏Å‡∏µ‡∏¢‡∏à‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà‡∏™‡∏∞‡∏î‡∏ß‡∏Å‡∏´‡∏≤‡∏£‡∏π‡∏õ‡∏Å‡πá‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏Ç‡πâ‡∏≤‡∏á‡∏•‡πà‡∏≤‡∏á‡∏ô‡∏µ‡πâ‡πÄ‡∏•‡∏¢‡∏ô‡πâ‡∏≤‡∏ö‡∏ö**\n\n\nV\nV\nV\nV\nV\nV\nV\nV\nV\nV")

sample_path = ("./sample_images") #folder sameple images
file_name = os.listdir(sample_path)
sample_image = st.sidebar.selectbox(   #create selectbox sidebar
    'Sample images',
    (file_name))

st.sidebar.title("**‡∏™‡πà‡∏á‡∏Ç‡πâ‡∏≠‡πÄ‡∏™‡∏ô‡∏≠‡πÅ‡∏ô‡∏∞‡∏ó‡∏µ‡πà‡∏ô‡∏µ‡πà‡πÄ‡∏•‡∏¢~:**")
st.sidebar.markdown("**Google form:**")
st.sidebar.markdown("https://forms.gle/dYj3TJ1mExdPRNQt8")
st.sidebar.markdown("-----------------")
st.sidebar.markdown("**Contact:**")
st.sidebar.markdown("Email: mhapongg@gmail.com")
st.sidebar.markdown("-----------------")
st.markdown("-----------------")
file = st.file_uploader("Upload your image:") #upload file
st.markdown("-----------------")
if file is None:
    img = PILImage.create(os.path.join(sample_path, sample_image))
    st.markdown('\n')
    st.title("Here is the sample image:") #display sample image
    st.image(img)

else:
    img = PILImage.create(file)
    st.title("Here is the image you've selected:") #display selected image
    st.image(img)
c_type = ['Altocumulus', 'Altostratus', 'Cirrocumulus', 'Cirrostratus', 'Cirrus', 'Contrails', 'Cumulonimbus', 'Cumulus', 'Nimbostratus', 'Stratocumulus', 'Stratus']
im_predicted = model.predict(img) #predict model
c_name = im_predicted[0]
ts_prob = im_predicted[2]
prob = torch.sort(ts_prob, descending=True)
m_prob = prob[0][0]

if c_name in c_type:
     st.spinner(text="In progress...")
     st.success(f"This cloud is **{c_name}**  with the probability of **{m_prob*100:.02f}**%") #result displays

     if c_name == c_type[0]:
         st.title(f"**{c_name}** \n ")
         st.header('**‡∏≠‡∏±‡∏•‡πÇ‡∏ï‡∏Ñ‡∏¥‡∏ß‡∏°‡∏π‡∏•‡∏±‡∏™ (‡πÄ‡∏°‡∏Ü‡∏ä‡∏±‡πâ‡∏ô‡∏Å‡∏•‡∏≤‡∏á):**')
         st.markdown('**‡πÄ‡∏õ‡πá‡∏ô‡πÄ‡∏°‡∏Ü‡∏ä‡∏±‡πâ‡∏ô‡∏Å‡∏•‡∏≤‡∏á:** ‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡∏∂‡πâ‡∏ô‡∏ó‡∏µ‡πà‡∏£‡∏∞‡∏î‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á 2-6 ‡∏Å‡∏¥‡πÇ‡∏•‡πÄ‡∏°‡∏ï‡∏£ ‡∏õ‡∏£‡∏∞‡∏Å‡∏≠‡∏ö‡∏î‡πâ‡∏ß‡∏¢‡∏ú‡∏•‡∏∂‡∏Å‡∏ô‡πâ‡∏≥‡πÅ‡∏Ç‡πá‡∏á‡πÅ‡∏•‡∏∞‡∏≠‡∏ô‡∏∏‡∏†‡∏≤‡∏Ñ‡∏ô‡πâ‡∏≥ ‡πÄ‡∏û‡∏£‡∏≤‡∏∞‡∏ó‡∏µ‡πà‡∏£‡∏∞‡∏î‡∏±‡∏ö‡∏ô‡∏µ‡πâ‡∏°‡∏µ‡∏≠‡∏∏‡∏ì‡∏´‡∏†‡∏π‡∏°‡∏¥‡πÑ‡∏°‡πà‡∏ï‡πà‡∏≥‡∏û‡∏≠‡∏ó‡∏µ‡πà‡∏à‡∏∞‡πÄ‡∏õ‡πá‡∏ô‡∏ú‡∏•‡∏∂‡∏Å.')
         st.markdown('‡πÄ‡∏°‡∏Ü‡∏ô‡∏µ‡πâ‡πÑ‡∏°‡πà‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÄ‡∏Å‡∏¥‡∏î‡∏ô‡πâ‡∏≥‡∏ü‡πâ‡∏≤‡∏ö‡πà‡∏á‡∏ö‡∏≠‡∏Å‡∏ß‡πà‡∏≤‡∏•‡∏±‡∏Å‡∏©‡∏ì‡∏∞‡∏≠‡∏≤‡∏Å‡∏≤‡∏®‡∏î‡∏µ‡πÇ‡∏î‡∏¢‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡∏´‡∏•‡∏±‡∏á‡∏û‡∏≤‡∏¢‡∏∏‡∏ù‡∏ô‡∏°‡∏µ‡∏•‡∏±‡∏Å‡∏©‡∏ì‡∏∞‡πÄ‡∏õ‡πá‡∏ô‡∏Å‡πâ‡∏≠‡∏ô‡πÄ‡∏•‡πá‡∏Å‡πÜ‡πÄ‡∏õ‡πá‡∏ô‡∏´‡∏¢‡πà‡∏≠‡∏°‡πÅ‡∏ú‡πà‡∏ô‡∏´‡∏£‡∏∑‡∏≠‡∏ä‡∏±‡πâ‡∏ô‡∏Ñ‡∏•‡πâ‡∏≤‡∏¢‡πÄ‡∏Å‡∏•‡πá‡∏î‡∏Å‡πâ‡∏≠‡∏ô‡∏Å‡∏•‡∏°‡∏´‡∏£‡∏∑‡∏≠‡∏°‡πâ‡∏ß‡∏ô‡∏°‡∏µ‡∏ó‡∏±‡πâ‡∏á‡∏™‡∏µ‡πÄ‡∏ó‡∏≤‡∏´‡∏£‡∏∑‡∏≠‡∏ó‡∏±‡πâ‡∏á‡∏™‡∏≠‡∏á‡∏™‡∏µ‡∏ö‡∏≤‡∏á‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏≠‡∏≤‡∏à‡πÄ‡∏´‡πá‡∏ô‡∏Ñ‡∏•‡πâ‡∏≤‡∏¢‡∏õ‡∏∏‡∏¢‡∏´‡∏£‡∏∑‡∏≠‡∏ù‡πâ‡∏≤‡∏ã‡∏∂‡πà‡∏á‡∏õ‡∏£‡∏∞‡∏Å‡∏≠‡∏ö‡∏î‡πâ‡∏ß‡∏¢‡∏•‡∏∞‡∏≠‡∏≠‡∏á‡∏ô‡πâ‡∏≥‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏°‡∏≤‡∏Å‡∏°‡∏±‡∏Å‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÄ‡∏Å‡∏¥‡∏î‡∏õ‡∏£‡∏≤‡∏Å‡∏è‡∏Å‡∏≤‡∏£‡∏ì‡πå‡∏ß‡∏á‡πÅ‡∏™‡∏á‡∏£‡∏≠‡∏ö‡∏î‡∏ß‡∏á‡∏≠‡∏≤‡∏ó‡∏¥‡∏ï‡∏¢‡πå‡∏´‡∏£‡∏∑‡∏≠‡∏î‡∏ß‡∏á‡∏à‡∏±‡∏ô‡∏ó‡∏£‡πå‡∏ó‡∏£‡∏á‡∏Å‡∏•‡∏î.')

     elif c_name == c_type[1]:
         st.title(f"**{c_name}** \n ")
         st.header('**‡∏≠‡∏±‡∏•‡πÇ‡∏ï‡∏™‡πÄ‡∏ï‡∏£‡∏ï‡∏±‡∏™ (‡πÄ‡∏°‡∏Ü‡∏ä‡∏±‡πâ‡∏ô‡∏Å‡∏•‡∏≤‡∏á):**')
         st.markdown('**‡πÄ‡∏õ‡πá‡∏ô‡πÄ‡∏°‡∏Ü‡∏ä‡∏±‡πâ‡∏ô‡∏Å‡∏•‡∏≤‡∏á:** ‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡∏∂‡πâ‡∏ô‡∏ó‡∏µ‡πà‡∏£‡∏∞‡∏î‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á 2-6 ‡∏Å‡∏¥‡πÇ‡∏•‡πÄ‡∏°‡∏ï‡∏£ ‡∏õ‡∏£‡∏∞‡∏Å‡∏≠‡∏ö‡∏î‡πâ‡∏ß‡∏¢‡∏ú‡∏•‡∏∂‡∏Å‡∏ô‡πâ‡∏≥‡πÅ‡∏Ç‡πá‡∏á‡πÅ‡∏•‡∏∞‡∏≠‡∏ô‡∏∏‡∏†‡∏≤‡∏Ñ‡∏ô‡πâ‡∏≥ ‡πÄ‡∏û‡∏£‡∏≤‡∏∞‡∏ó‡∏µ‡πà‡∏£‡∏∞‡∏î‡∏±‡∏ö‡∏ô‡∏µ‡πâ‡∏°‡∏µ‡∏≠‡∏∏‡∏ì‡∏´‡∏†‡∏π‡∏°‡∏¥‡πÑ‡∏°‡πà‡∏ï‡πà‡∏≥‡∏û‡∏≠‡∏ó‡∏µ‡πà‡∏à‡∏∞‡πÄ‡∏õ‡πá‡∏ô‡∏ú‡∏•‡∏∂‡∏Å.')
         st.markdown('‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏´‡πá‡∏ô‡πÄ‡∏°‡∏Ü‡∏ä‡∏ô‡∏¥‡∏î‡∏ô‡∏µ‡πâ‡∏≠‡∏≤‡∏à‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÄ‡∏Å‡∏¥‡∏î‡∏ù‡∏ô ‡∏´‡∏¥‡∏°‡∏∞ ‡∏´‡∏£‡∏∑‡∏≠‡∏•‡∏π‡∏Å‡∏õ‡∏£‡∏≤‡∏¢‡∏ô‡πâ‡∏≥‡πÅ‡∏Ç‡πá‡∏á ‡πÇ‡∏î‡∏¢‡∏à‡∏∞‡∏°‡∏µ‡∏•‡∏±‡∏Å‡∏©‡∏ì‡∏∞‡πÄ‡∏õ‡πá‡∏ô‡∏õ‡∏∏‡∏¢ ‡πÅ‡∏ú‡πà‡∏ô ‡∏´‡∏£‡∏∑‡∏≠‡πÄ‡∏ô‡∏∑‡πâ‡∏≠‡πÄ‡∏î‡∏µ‡∏¢‡∏ß‡∏Å‡∏±‡∏ô ‡∏û‡∏ö‡πÑ‡∏î‡πâ‡∏ó‡∏±‡πâ‡∏á‡∏™‡∏µ‡πÄ‡∏ó‡∏≤‡∏´‡∏£‡∏∑‡∏≠‡∏™‡∏µ‡∏ü‡πâ‡∏≤‡∏≠‡πà‡∏≠‡∏ô ‡∏≠‡∏≤‡∏à‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏°‡∏≠‡∏á‡∏î‡∏ß‡∏á‡∏≠‡∏≤‡∏ó‡∏¥‡∏ï‡∏¢‡πå‡∏à‡∏∞‡πÄ‡∏´‡πá‡∏ô‡πÑ‡∏î‡πâ‡πÅ‡∏ö‡∏ö‡∏™‡∏•‡∏±‡∏ß‡πÜ ‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡∏°‡∏≠‡∏á‡∏ú‡πà‡∏≤‡∏ô‡∏Å‡∏£‡∏∞‡∏à‡∏Å‡∏ù‡πâ‡∏≤')

     elif c_name == c_type[2]:
         st.title(f"**{c_name}** \n ")
         st.header('**‡πÄ‡∏°‡∏Ü‡πÄ‡∏ã‡∏≠‡πÇ‡∏£‡∏Ñ‡∏¥‡∏ß‡∏°‡∏π‡∏•‡∏±‡∏™ ‡∏´‡∏£‡∏∑‡∏≠‡∏ã‡∏µ‡∏£‡πå‡πÇ‡∏£‡∏Ñ‡∏¥‡∏ß‡∏°‡∏π‡∏•‡∏±‡∏™ (‡πÄ‡∏°‡∏Ü‡∏ä‡∏±‡πâ‡∏ô‡∏™‡∏π‡∏á):**')
         st.markdown('**‡πÄ‡∏õ‡πá‡∏ô‡πÄ‡∏°‡∏Ü‡∏ä‡∏±‡πâ‡∏ô‡∏™‡∏π‡∏á:** ‡πÉ‡∏ô‡∏ö‡∏£‡∏¥‡πÄ‡∏ß‡∏ì‡πÄ‡∏Ç‡∏ï‡∏£‡πâ‡∏≠‡∏ô‡∏à‡∏∞‡πÄ‡∏Å‡∏¥‡∏î‡∏ó‡∏µ‡πà‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏£‡∏∞‡∏´‡∏ß‡πà‡∏≤‡∏á 6-12 ‡∏Å‡∏¥‡πÇ‡∏•‡πÄ‡∏°‡∏ï‡∏£ ‡∏™‡πà‡∏ß‡∏ô‡πÉ‡∏´‡∏ç‡πà‡∏à‡∏∞‡∏°‡∏µ‡∏™‡∏µ‡∏Ç‡∏≤‡∏ß‡∏´‡∏£‡∏∑‡∏≠‡πÄ‡∏ó‡∏≤‡∏≠‡πà‡∏≠‡∏ô ‡πÅ‡∏•‡∏∞‡πÄ‡∏õ‡πá‡∏ô‡πÄ‡∏°‡∏Ü‡∏ã‡∏∂‡πà‡∏á‡πÑ‡∏°‡πà‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÄ‡∏Å‡∏¥‡∏î‡∏ù‡∏ô ‡∏™‡πà‡∏ß‡∏ô‡πÉ‡∏´‡∏ç‡πà‡∏à‡∏∞‡πÄ‡∏õ‡πá‡∏ô‡∏ú‡∏•‡∏∂‡∏Å‡∏ô‡πâ‡∏≥‡πÅ‡∏Ç‡πá‡∏á ‡πÄ‡∏û‡∏£‡∏≤‡∏∞‡∏°‡∏µ‡∏≠‡∏∏‡∏ì‡∏´‡∏†‡∏π‡∏°‡∏¥‡∏ï‡πà‡∏≥‡∏Å‡∏ß‡πà‡∏≤‡∏à‡∏∏‡∏î‡πÄ‡∏¢‡∏∑‡∏≠‡∏Å‡πÅ‡∏Ç‡πá‡∏á ‡πÅ‡∏•‡∏∞‡∏°‡∏µ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÅ‡∏õ‡∏£‡∏õ‡∏£‡∏ß‡∏ô.')
         st.markdown('‡πÄ‡∏°‡∏Ü‡∏•‡∏±‡∏Å‡∏©‡∏ì‡∏∞‡∏ô‡∏µ‡πâ‡∏Å‡πá‡∏ö‡πà‡∏á‡∏ö‡∏≠‡∏Å‡∏•‡∏±‡∏Å‡∏©‡∏ì‡∏∞‡∏≠‡∏≤‡∏Å‡∏≤‡∏®‡∏î‡∏µ‡πÄ‡∏ä‡πà‡∏ô‡∏Å‡∏±‡∏ô‡∏•‡∏±‡∏Å‡∏©‡∏ì‡∏∞‡πÄ‡∏õ‡πá‡∏ô‡∏´‡∏¢‡πà‡∏≠‡∏°,‡πÅ‡∏ú‡πà‡∏ô,‡∏´‡∏£‡∏∑‡∏≠‡∏ä‡∏±‡πâ‡∏ô‡∏ö‡∏≤‡∏á‡πÜ ‡∏™‡∏µ‡∏Ç‡∏≤‡∏ß ‡∏Ñ‡∏•‡πâ‡∏≤‡∏¢‡πÄ‡∏°‡∏Ü‡∏Å‡πâ‡∏≠‡∏ô‡πÄ‡∏•‡πá‡∏Å‡πÜ ‡∏°‡∏µ‡∏ó‡∏±‡πâ‡∏á‡∏ó‡∏µ‡πà‡∏≠‡∏¢‡∏π‡πà‡∏Ñ‡∏¥‡∏î‡∏Å‡∏±‡∏ô‡πÅ‡∏•‡∏∞‡πÅ‡∏¢‡∏Å‡∏Å‡∏±‡∏ô‡πÄ‡∏£‡∏µ‡∏¢‡∏á‡∏ï‡∏±‡∏ß‡∏≠‡∏¢‡πà‡∏≤‡∏á‡πÄ‡∏õ‡πá‡∏ô‡∏£‡∏∞‡πÄ‡∏ö‡∏µ‡∏¢‡∏ö‡πÄ‡∏°‡∏Ü‡∏ä‡∏ô‡∏¥‡∏î‡∏ô‡∏µ‡πâ‡∏≠‡∏≤‡∏à‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÄ‡∏Å‡∏¥‡∏î‡∏õ‡∏£‡∏≤‡∏Å‡∏è‡∏Å‡∏≤‡∏£‡∏ì‡πå‡∏ß‡∏á‡πÅ‡∏™‡∏á‡∏£‡∏≠‡∏ö‡∏î‡∏ß‡∏á‡∏≠‡∏≤‡∏ó‡∏¥‡∏ï‡∏¢‡πå‡∏´‡∏£‡∏∑‡∏≠‡∏î‡∏ß‡∏á‡∏à‡∏±‡∏ô‡∏ó‡∏£‡πå‡∏ó‡∏£‡∏á‡∏Å‡∏•‡∏î‡πÅ‡∏•‡∏∞‡∏õ‡∏£‡∏≤‡∏Å‡∏è‡∏Å‡∏≤‡∏£‡∏ì‡πå‡πÅ‡∏ñ‡∏ö‡∏™‡∏µ‡∏´‡∏£‡∏∑‡∏≠‡∏£‡∏∏‡πâ‡∏á‡∏õ‡∏£‡∏∞‡∏Å‡∏≠‡∏ö‡∏î‡πâ‡∏ß‡∏¢‡∏ú‡∏•‡∏∂‡∏Å‡∏ô‡πâ‡∏≥‡πÅ‡∏Ç‡πá‡∏á ‡∏ê‡∏≤‡∏ô‡πÄ‡∏°‡∏Ü‡∏™‡∏π‡∏á‡πÄ‡∏â‡∏•‡∏µ‡πà‡∏¢‡∏õ‡∏£‡∏∞‡∏°‡∏≤‡∏ì 7,000 ‡πÄ‡∏°‡∏ï‡∏£')
         
     elif c_name == c_type[3]:
         st.title(f"**{c_name}** \n ")
         st.header('**‡πÄ‡∏°‡∏Ü‡πÄ‡∏ã‡∏≠‡πÇ‡∏£‡∏™‡πÄ‡∏ï‡∏£‡∏ï‡∏±‡∏™ ‡∏´‡∏£‡∏∑‡∏≠‡∏ã‡∏µ‡∏£‡πå‡πÇ‡∏£‡∏™‡πÄ‡∏ï‡∏£‡∏ï‡∏±‡∏™ (‡πÄ‡∏°‡∏Ü‡∏ä‡∏±‡πâ‡∏ô‡∏™‡∏π‡∏á):**')
         st.markdown('**‡πÄ‡∏õ‡πá‡∏ô‡πÄ‡∏°‡∏Ü‡∏ä‡∏±‡πâ‡∏ô‡∏™‡∏π‡∏á:** ‡πÉ‡∏ô‡∏ö‡∏£‡∏¥‡πÄ‡∏ß‡∏ì‡πÄ‡∏Ç‡∏ï‡∏£‡πâ‡∏≠‡∏ô‡∏à‡∏∞‡πÄ‡∏Å‡∏¥‡∏î‡∏ó‡∏µ‡πà‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏£‡∏∞‡∏´‡∏ß‡πà‡∏≤‡∏á 6-12 ‡∏Å‡∏¥‡πÇ‡∏•‡πÄ‡∏°‡∏ï‡∏£ ‡∏™‡πà‡∏ß‡∏ô‡πÉ‡∏´‡∏ç‡πà‡∏à‡∏∞‡∏°‡∏µ‡∏™‡∏µ‡∏Ç‡∏≤‡∏ß‡∏´‡∏£‡∏∑‡∏≠‡πÄ‡∏ó‡∏≤‡∏≠‡πà‡∏≠‡∏ô ‡πÅ‡∏•‡∏∞‡πÄ‡∏õ‡πá‡∏ô‡πÄ‡∏°‡∏Ü‡∏ã‡∏∂‡πà‡∏á‡πÑ‡∏°‡πà‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÄ‡∏Å‡∏¥‡∏î‡∏ù‡∏ô ‡∏™‡πà‡∏ß‡∏ô‡πÉ‡∏´‡∏ç‡πà‡∏à‡∏∞‡πÄ‡∏õ‡πá‡∏ô‡∏ú‡∏•‡∏∂‡∏Å‡∏ô‡πâ‡∏≥‡πÅ‡∏Ç‡πá‡∏á ‡πÄ‡∏û‡∏£‡∏≤‡∏∞‡∏°‡∏µ‡∏≠‡∏∏‡∏ì‡∏´‡∏†‡∏π‡∏°‡∏¥‡∏ï‡πà‡∏≥‡∏Å‡∏ß‡πà‡∏≤‡∏à‡∏∏‡∏î‡πÄ‡∏¢‡∏∑‡∏≠‡∏Å‡πÅ‡∏Ç‡πá‡∏á ‡πÅ‡∏•‡∏∞‡∏°‡∏µ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÅ‡∏õ‡∏£‡∏õ‡∏£‡∏ß‡∏ô.')
         st.markdown('‡πÄ‡∏°‡∏Ü‡∏ô‡∏µ‡πâ‡πÑ‡∏°‡πà‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÄ‡∏Å‡∏¥‡∏î‡∏ô‡πâ‡∏≥‡∏ü‡πâ‡∏≤‡∏ö‡πà‡∏á‡∏ö‡∏≠‡∏Å‡∏ß‡πà‡∏≤‡∏•‡∏±‡∏Å‡∏©‡∏ì‡∏∞‡∏≠‡∏≤‡∏Å‡∏≤‡∏®‡∏î‡∏µ‡πÇ‡∏î‡∏¢‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡∏´‡∏•‡∏±‡∏á‡∏û‡∏≤‡∏¢‡∏∏‡∏ù‡∏ô‡∏°‡∏µ‡∏•‡∏±‡∏Å‡∏©‡∏ì‡∏∞‡πÄ‡∏õ‡πá‡∏ô‡∏Å‡πâ‡∏≠‡∏ô‡πÄ‡∏•‡πá‡∏Å‡πÜ‡πÄ‡∏õ‡πá‡∏ô‡∏´‡∏¢‡πà‡∏≠‡∏°‡πÅ‡∏ú‡πà‡∏ô‡∏´‡∏£‡∏∑‡∏≠‡∏ä‡∏±‡πâ‡∏ô‡∏Ñ‡∏•‡πâ‡∏≤‡∏¢‡πÄ‡∏Å‡∏•‡πá‡∏î‡∏Å‡πâ‡∏≠‡∏ô‡∏Å‡∏•‡∏°‡∏´‡∏£‡∏∑‡∏≠‡∏°‡πâ‡∏ß‡∏ô‡∏°‡∏µ‡∏ó‡∏±‡πâ‡∏á‡∏™‡∏µ‡πÄ‡∏ó‡∏≤‡∏´‡∏£‡∏∑‡∏≠‡∏ó‡∏±‡πâ‡∏á‡∏™‡∏≠‡∏á‡∏™‡∏µ‡∏ö‡∏≤‡∏á‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏≠‡∏≤‡∏à‡πÄ‡∏´‡πá‡∏ô‡∏Ñ‡∏•‡πâ‡∏≤‡∏¢‡∏õ‡∏∏‡∏¢‡∏´‡∏£‡∏∑‡∏≠‡∏ù‡πâ‡∏≤‡∏ã‡∏∂‡πà‡∏á‡∏õ‡∏£‡∏∞‡∏Å‡∏≠‡∏ö‡∏î‡πâ‡∏ß‡∏¢‡∏•‡∏∞‡∏≠‡∏≠‡∏á‡∏ô‡πâ‡∏≥‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏°‡∏≤‡∏Å‡∏°‡∏±‡∏Å‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÄ‡∏Å‡∏¥‡∏î‡∏õ‡∏£‡∏≤‡∏Å‡∏è‡∏Å‡∏≤‡∏£‡∏ì‡πå‡∏ß‡∏á‡πÅ‡∏™‡∏á‡∏£‡∏≠‡∏ö‡∏î‡∏ß‡∏á‡∏≠‡∏≤‡∏ó‡∏¥‡∏ï‡∏¢‡πå‡∏´‡∏£‡∏∑‡∏≠‡∏î‡∏ß‡∏á‡∏à‡∏±‡∏ô‡∏ó‡∏£‡πå‡∏ó‡∏£‡∏á‡∏Å‡∏•‡∏î‡∏õ‡∏£‡∏∞‡∏Å‡∏≠‡∏ö‡∏î‡πâ‡∏ß‡∏¢‡∏ú‡∏•‡∏∂‡∏Å‡∏ô‡πâ‡∏≥‡πÅ‡∏Ç‡πá‡∏á ‡πÄ‡∏õ‡πá‡∏ô‡πÄ‡∏°‡∏Ü‡∏ó‡∏µ‡πà‡πÄ‡∏õ‡πá‡∏ô‡∏£‡∏∞‡∏•‡∏≠‡∏Å‡∏Ñ‡∏•‡∏∑‡πà‡∏ô‡∏´‡∏£‡∏∑‡∏≠‡∏Å‡πâ‡∏≠‡∏ô‡∏Å‡∏•‡∏°‡πÜ ‡πÄ‡∏õ‡πá‡∏ô‡πÅ‡∏ñ‡∏ß‡πÜ ‡∏•‡∏±‡∏Å‡∏©‡∏ì‡∏∞‡∏Ñ‡∏•‡πâ‡∏≤‡∏¢‡πÄ‡∏Å‡∏•‡πá‡∏î‡∏õ‡∏•‡∏≤ ‡πÄ‡∏£‡∏µ‡∏¢‡∏á‡∏Å‡∏±‡∏ô‡πÄ‡∏õ‡πá‡∏ô‡∏£‡∏∞‡πÄ‡∏ö‡∏µ‡∏¢‡∏ö ‡∏ê‡∏≤‡∏ô‡πÄ‡∏°‡∏Ü‡∏™‡∏π‡∏á‡πÄ‡∏â‡∏•‡∏µ‡πà‡∏¢‡∏õ‡∏£‡∏∞‡∏°‡∏≤‡∏ì 7,000 ‡πÄ‡∏°‡∏ï‡∏£.')
         
     elif c_name == c_type[4]:
         st.title(f"**{c_name}** \n ")
         st.header('**‡πÄ‡∏°‡∏Ü‡πÄ‡∏ã‡∏≠‡∏£‡∏±‡∏™ ‡∏´‡∏£‡∏∑‡∏≠‡∏ã‡∏µ‡∏£‡πå‡∏£‡∏±‡∏™ (‡πÄ‡∏°‡∏Ü‡∏ä‡∏±‡πâ‡∏ô‡∏™‡∏π‡∏á):**')
         st.markdown('**‡πÄ‡∏õ‡πá‡∏ô‡πÄ‡∏°‡∏Ü‡∏ä‡∏±‡πâ‡∏ô‡∏™‡∏π‡∏á:** ‡πÉ‡∏ô‡∏ö‡∏£‡∏¥‡πÄ‡∏ß‡∏ì‡πÄ‡∏Ç‡∏ï‡∏£‡πâ‡∏≠‡∏ô‡∏à‡∏∞‡πÄ‡∏Å‡∏¥‡∏î‡∏ó‡∏µ‡πà‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏£‡∏∞‡∏´‡∏ß‡πà‡∏≤‡∏á 6-12 ‡∏Å‡∏¥‡πÇ‡∏•‡πÄ‡∏°‡∏ï‡∏£ ‡∏™‡πà‡∏ß‡∏ô‡πÉ‡∏´‡∏ç‡πà‡∏à‡∏∞‡∏°‡∏µ‡∏™‡∏µ‡∏Ç‡∏≤‡∏ß‡∏´‡∏£‡∏∑‡∏≠‡πÄ‡∏ó‡∏≤‡∏≠‡πà‡∏≠‡∏ô ‡πÅ‡∏•‡∏∞‡πÄ‡∏õ‡πá‡∏ô‡πÄ‡∏°‡∏Ü‡∏ã‡∏∂‡πà‡∏á‡πÑ‡∏°‡πà‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÄ‡∏Å‡∏¥‡∏î‡∏ù‡∏ô ‡∏™‡πà‡∏ß‡∏ô‡πÉ‡∏´‡∏ç‡πà‡∏à‡∏∞‡πÄ‡∏õ‡πá‡∏ô‡∏ú‡∏•‡∏∂‡∏Å‡∏ô‡πâ‡∏≥‡πÅ‡∏Ç‡πá‡∏á ‡πÄ‡∏û‡∏£‡∏≤‡∏∞‡∏°‡∏µ‡∏≠‡∏∏‡∏ì‡∏´‡∏†‡∏π‡∏°‡∏¥‡∏ï‡πà‡∏≥‡∏Å‡∏ß‡πà‡∏≤‡∏à‡∏∏‡∏î‡πÄ‡∏¢‡∏∑‡∏≠‡∏Å‡πÅ‡∏Ç‡πá‡∏á ‡πÅ‡∏•‡∏∞‡∏°‡∏µ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÅ‡∏õ‡∏£‡∏õ‡∏£‡∏ß‡∏ô.')
         st.markdown('‡∏´‡∏≤‡∏Å‡πÄ‡∏´‡πá‡∏ô‡πÄ‡∏°‡∏Ü‡∏õ‡∏£‡∏∞‡πÄ‡∏†‡∏ó‡∏ô‡∏µ‡πâ‡∏ö‡πà‡∏á‡∏ö‡∏≠‡∏Å‡πÑ‡∏î‡πâ‡πÄ‡∏•‡∏¢‡∏ß‡πà‡∏≤‡∏ß‡∏±‡∏ô‡∏ô‡∏±‡πâ‡∏ô‡∏•‡∏±‡∏Å‡∏©‡∏ì‡∏∞‡∏≠‡∏≤‡∏Å‡∏≤‡∏®‡∏î‡∏µ ‡πÄ‡∏°‡∏Ü‡∏ä‡∏ô‡∏¥‡∏î‡∏ô‡∏µ‡πâ‡∏≠‡∏≤‡∏à‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÄ‡∏Å‡∏¥‡∏î‡∏õ‡∏£‡∏≤‡∏Å‡∏è‡∏Å‡∏≤‡∏£‡∏ì‡πå‡∏ß‡∏á‡πÅ‡∏™‡∏á‡∏£‡∏≠‡∏ö‡∏î‡∏ß‡∏á‡∏≠‡∏≤‡∏ó‡∏¥‡∏ï‡∏¢‡πå‡∏´‡∏£‡∏∑‡∏≠‡∏î‡∏ß‡∏á‡∏à‡∏±‡∏ô‡∏ó‡∏£‡πå‡πÅ‡∏ï‡πà‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡πÄ‡∏ï‡πá‡∏°‡∏ß‡∏á ‡∏õ‡∏£‡∏∞‡∏Å‡∏≠‡∏ö‡∏î‡πâ‡∏ß‡∏¢‡∏ú‡∏•‡∏∂‡∏Å‡∏ô‡πâ‡∏≥‡πÅ‡∏Ç‡πá‡∏á ‡∏°‡∏µ‡∏•‡∏±‡∏Å‡∏©‡∏ì‡∏∞‡πÄ‡∏õ‡πá‡∏ô‡πÅ‡∏ú‡πà‡∏ô‡∏ö‡∏≤‡∏á‡∏™‡∏µ‡∏Ç‡∏≤‡∏ß‡πÄ‡∏à‡∏¥‡∏î‡∏à‡πâ‡∏≤ ‡∏´‡∏£‡∏∑‡∏≠‡∏™‡∏µ‡πÄ‡∏ó‡∏≤‡∏≠‡πà‡∏≠‡∏ô ‡∏°‡∏µ‡∏£‡∏π‡∏õ‡∏ó‡∏£‡∏á‡∏≠‡∏¢‡∏π‡πà‡∏´‡∏•‡∏≤‡∏¢‡πÅ‡∏ö‡∏ö ‡πÄ‡∏ä‡πà‡∏ô ‡πÄ‡∏õ‡πá‡∏ô‡∏ù‡∏≠‡∏¢‡∏Ñ‡∏•‡πâ‡∏≤‡∏¢‡∏Ç‡∏ô‡∏ô‡∏Å‡∏ö‡∏≤‡∏á‡πÜ ‡∏´‡∏£‡∏∑‡∏≠‡∏Ç‡∏ô‡∏™‡∏±‡∏ï‡∏ß‡πå ‡∏´‡∏£‡∏∑‡∏≠‡πÄ‡∏õ‡πá‡∏ô‡∏ó‡∏≤‡∏á‡∏¢‡∏≤‡∏ß ‡∏ê‡∏≤‡∏ô‡πÄ‡∏°‡∏Ü‡∏™‡∏π‡∏á‡πÄ‡∏â‡∏•‡∏µ‡πà‡∏¢ 10,000 ‡πÄ‡∏°‡∏ï‡∏£.')
         
     elif c_name == c_type[5]:
         st.title(f"**{c_name}** \n **‡πÄ‡∏°‡∏Ü‡∏´‡∏≤‡∏á‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏ö‡∏¥‡∏ô:**\n")
         st.markdown('**‡∏Ñ‡∏≠‡∏ô‡πÄ‡∏ó‡∏£‡∏•:** ‡∏°‡∏µ‡∏™‡∏≠‡∏á‡∏ä‡∏ô‡∏¥‡∏î‡∏ó‡∏±‡πâ‡∏á‡∏™‡∏≠‡∏á‡∏ä‡∏ô‡∏¥‡∏î‡∏•‡πâ‡∏ß‡∏ô‡πÄ‡∏Å‡∏¥‡∏î‡∏à‡∏≤‡∏Å‡∏Å‡∏≤‡∏£‡∏Ñ‡∏ß‡∏ö‡πÅ‡∏ô‡πà‡∏ô‡∏´‡∏£‡∏∑‡∏≠‡∏Å‡∏≤‡∏£‡∏Å‡∏•‡∏±‡πà‡∏ô‡∏ï‡∏±‡∏ß‡∏Ç‡∏≠‡∏á‡πÑ‡∏≠‡∏ô‡πâ‡∏≥‡πÉ‡∏ô‡∏≠‡∏≤‡∏Å‡∏≤‡∏®‡πÑ‡∏õ‡πÄ‡∏õ‡πá‡∏ô‡∏´‡∏¢‡∏î‡∏ô‡πâ‡∏≥‡πÄ‡∏•‡πá‡∏Å‡πÜ‡πÉ‡∏ô‡∏£‡∏π‡∏õ‡πÅ‡∏ö‡∏ö‡πÄ‡∏î‡∏µ‡∏¢‡∏ß‡∏Å‡∏±‡∏ö‡πÄ‡∏°‡∏Ü‡∏ô‡∏±‡πà‡∏ô‡πÄ‡∏≠‡∏á ‡∏Ñ‡∏∑‡∏≠')
         st.markdown('**‡∏Ñ‡∏≠‡∏ô‡πÄ‡∏ó‡∏£‡∏•‡∏ó‡∏µ‡πà‡πÄ‡∏Å‡∏¥‡∏î‡∏à‡∏≤‡∏Å‡πÑ‡∏≠‡∏û‡πà‡∏ô (exhaustcontrails):** ‡πÑ‡∏≠‡∏£‡πâ‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡∏ñ‡∏π‡∏Å‡∏û‡πà‡∏ô‡∏≠‡∏≠‡∏Å‡∏°‡∏≤‡∏õ‡∏∞‡∏ó‡∏∞‡∏Å‡∏±‡∏ö‡∏≠‡∏≤‡∏Å‡∏≤‡∏®‡∏ó‡∏µ‡πà‡∏≠‡∏∏‡∏ì‡∏´‡∏†‡∏π‡∏°‡∏¥‡∏ï‡πà‡∏≥‡πÅ‡∏•‡∏∞‡∏Å‡∏•‡∏±‡πà‡∏ô‡∏ï‡∏±‡∏ß‡πÄ‡∏õ‡πá‡∏ô‡∏´‡∏¢‡∏î‡∏ô‡πâ‡∏≥‡πÉ‡∏ô‡∏™‡∏∏‡∏î ‡πÄ‡∏£‡∏≤‡∏à‡∏∂‡∏á‡∏û‡∏ö‡∏Ñ‡∏≠‡∏ô‡πÄ‡∏ó‡∏£‡∏•‡πÑ‡∏î‡πâ‡∏ö‡πà‡∏≠‡∏¢‡∏Ñ‡∏£‡∏±‡πâ‡∏á ‡πÉ‡∏ô‡∏Ç‡∏ì‡∏∞‡∏ó‡∏µ‡πà‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏ö‡∏¥‡∏ô‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ö‡∏¥‡∏ô‡∏≠‡∏¢‡∏π‡πà‡∏ö‡∏ô‡∏£‡∏∞‡∏î‡∏±‡∏ö‡πÄ‡∏û‡∏î‡∏≤‡∏ô‡∏ö‡∏¥‡∏ô‡∏ó‡∏µ‡πà‡∏Ñ‡πà‡∏≠‡∏ô‡∏Ç‡πâ‡∏≤‡∏á‡∏™‡∏π‡∏á‡πÄ‡∏û‡∏£‡∏≤‡∏∞‡∏ß‡πà‡∏≤‡∏ó‡∏µ‡πà‡∏£‡∏∞‡∏î‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏°‡∏≤‡∏Å‡πÜ ‡∏≠‡∏∏‡∏ì‡∏´‡∏†‡∏π‡∏°‡∏¥‡∏Ç‡∏≠‡∏á‡∏≠‡∏≤‡∏Å‡∏≤‡∏®‡∏£‡∏≠‡∏ö‡πÜ‡∏à‡∏∞‡∏•‡∏î‡∏ï‡πà‡∏≥‡∏•‡∏á')
         st.markdown('**‡∏Ñ‡∏≠‡∏ô‡πÄ‡∏ó‡∏£‡∏•‡∏ó‡∏µ‡πà‡πÄ‡∏Å‡∏¥‡∏î‡∏à‡∏≤‡∏Å‡∏´‡∏•‡∏±‡∏Å‡∏≠‡∏≤‡∏Å‡∏≤‡∏®‡∏û‡∏•‡∏®‡∏≤‡∏™‡∏ï‡∏£‡πå (aerodynamic contrails):** ‡∏Ñ‡∏≠‡∏ô‡πÄ‡∏ó‡∏£‡∏•‡∏ä‡∏ô‡∏¥‡∏î‡∏ô‡∏µ‡πâ‡∏à‡∏∞‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡∏∂‡πâ‡∏ô‡∏ó‡∏µ‡πà‡πÑ‡∏´‡∏ô‡∏Å‡πá‡πÑ‡∏î‡πâ‡∏ó‡∏µ‡πà‡∏≠‡∏≤‡∏Å‡∏≤‡∏®‡∏°‡∏µ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏ä‡∏∑‡πâ‡∏ô‡∏°‡∏≤‡∏Å‡πÅ‡∏•‡∏∞‡∏Ñ‡∏ß‡∏≤‡∏°‡∏Å‡∏î‡∏≠‡∏≤‡∏Å‡∏≤‡∏®‡∏ï‡πà‡∏≥‡∏°‡∏≤‡∏Å‡∏û‡∏≠ ‡πÅ‡∏ï‡πà‡∏ó‡∏µ‡πà‡πÜ‡πÄ‡∏£‡∏≤‡∏à‡∏∞‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏û‡∏ö‡πÑ‡∏î‡πâ‡∏°‡∏≤‡∏Å‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î‡∏Å‡πá‡∏Ñ‡∏∑‡∏≠‡∏ö‡∏£‡∏¥‡πÄ‡∏ß‡∏ì‡πÄ‡∏´‡∏ô‡∏∑‡∏≠‡∏õ‡∏µ‡∏Å‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏ö‡∏¥‡∏ô (airfoils) ‡∏Ç‡∏ì‡∏∞‡∏ó‡∏µ‡πà‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏ö‡∏¥‡∏ô‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏•‡∏á‡∏à‡∏≠‡∏î (landing) ‡πÄ‡∏ô‡∏∑‡πà‡∏≠‡∏á‡∏à‡∏≤‡∏Å‡∏ö‡∏£‡∏¥‡πÄ‡∏ß‡∏ì‡πÄ‡∏´‡∏ô‡∏∑‡∏≠‡∏õ‡∏µ‡∏Å‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏ö‡∏¥‡∏ô‡∏à‡∏∞‡∏°‡∏µ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏Å‡∏î‡∏≠‡∏≤‡∏Å‡∏≤‡∏®‡∏ï‡πà‡∏≥‡∏•‡∏á‡πÄ‡∏õ‡πá‡∏ô‡∏û‡∏¥‡πÄ‡∏®‡∏© ‡∏ã‡∏∂‡πà‡∏á‡πÄ‡∏õ‡πá‡∏ô‡∏ú‡∏•‡∏°‡∏≤‡∏à‡∏≤‡∏Å‡∏Å‡∏≤‡∏£‡∏≠‡∏≠‡∏Å‡πÅ‡∏ö‡∏ö‡∏£‡∏π‡∏õ‡∏ó‡∏£‡∏á ‡∏Ç‡∏≠‡∏á‡∏õ‡∏µ‡∏Å‡πÅ‡∏•‡∏∞ ‡∏°‡∏∏‡∏°‡∏õ‡∏∞‡∏ó‡∏∞‡∏£‡∏∞‡∏´‡∏ß‡πà‡∏≤‡∏á‡∏•‡∏°‡πÅ‡∏•‡∏∞‡∏õ‡∏µ‡∏Å‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏ö‡∏¥‡∏ô (angle of attack) ‡πÉ‡∏ô‡∏£‡∏∞‡∏´‡∏ß‡πà‡∏≤‡∏á‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏Å‡∏≤‡∏£‡∏ö‡∏¥‡∏ô ‡πÇ‡∏î‡∏¢‡∏Ñ‡∏≠‡∏ô‡πÄ‡∏ó‡∏£‡∏•‡∏ä‡∏ô‡∏¥‡∏î‡∏ô‡∏µ‡πâ‡∏à‡∏∞‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡∏∂‡πâ‡∏ô‡∏°‡∏≤‡πÅ‡∏•‡∏∞‡∏´‡∏≤‡∏¢‡πÑ‡∏õ‡∏ó‡∏±‡∏ô‡∏ó‡∏µ‡∏ó‡∏µ‡πà‡∏≠‡∏≤‡∏Å‡∏≤‡∏®‡∏Å‡∏•‡∏±‡∏ö‡πÄ‡∏Ç‡πâ‡∏≤‡∏™‡∏π‡πà‡∏≠‡∏∏‡∏ì‡∏´‡∏†‡∏π‡∏°‡∏¥‡∏õ‡∏Å‡∏ï‡∏¥ (ambient temperature) ')
         st.image('./PContrail.jpg')

     elif c_name == c_type[6]:
         st.title(f"**{c_name}** \n")
         st.header('**‡πÄ‡∏°‡∏Ü‡∏Ñ‡∏¥‡∏ß‡∏°‡∏π‡πÇ‡∏•‡∏ô‡∏¥‡∏°‡∏ö‡∏±‡∏™(‡πÄ‡∏°‡∏Ü‡∏Å‡πà‡∏≠‡∏ï‡∏±‡∏ß‡πÉ‡∏ô‡πÅ‡∏ô‡∏ß‡∏ï‡∏±‡πâ‡∏á):**')
         st.markdown('**‡πÄ‡∏°‡∏Ü‡∏Å‡πà‡∏≠‡∏ï‡∏±‡∏ß‡πÉ‡∏ô‡πÅ‡∏ô‡∏ß‡∏ï‡∏±‡πâ‡∏á:** ‡πÄ‡∏õ‡πá‡∏ô‡πÄ‡∏°‡∏Ü‡∏ó‡∏µ‡πà‡∏Å‡πà‡∏≠‡∏ï‡∏±‡∏ß‡πÉ‡∏ô‡πÅ‡∏ô‡∏ß‡∏ï‡∏±‡πâ‡∏á‡∏ó‡∏µ‡πà‡∏°‡∏µ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏£‡∏∏‡∏ô‡πÅ‡∏£‡∏á ‡πÅ‡∏•‡∏∞‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡∏∂‡πâ‡∏ô‡πÇ‡∏î‡∏¢‡∏â‡∏±‡∏ö‡∏û‡∏•‡∏±‡∏ô ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏Ç‡∏≠‡∏á‡∏ê‡∏≤‡∏ô‡πÄ‡∏°‡∏Ü‡∏≠‡∏¢‡∏π‡πà‡∏ó‡∏µ‡πà‡∏õ‡∏£‡∏∞‡∏°‡∏≤‡∏ì 500 ‡πÄ‡∏°‡∏ï‡∏£ ‡∏™‡πà‡∏ß‡∏ô‡∏¢‡∏≠‡∏î‡πÄ‡∏°‡∏Ü‡∏°‡∏µ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÑ‡∏°‡πà‡πÅ‡∏ô‡πà‡∏ô‡∏≠‡∏ô ‡∏ö‡∏≤‡∏á‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏≠‡∏≤‡∏à‡∏™‡∏π‡∏á‡∏ñ‡∏∂‡∏á‡∏£‡∏∞‡∏î‡∏±‡∏ö‡πÄ‡∏°‡∏Ü‡∏ä‡∏±‡πâ‡∏ô‡∏™‡∏π‡∏á')
         st.markdown('‡∏°‡∏µ‡∏•‡∏±‡∏Å‡∏©‡∏ì‡∏∞‡πÄ‡∏õ‡πá‡∏ô‡πÄ‡∏°‡∏Ü‡∏ó‡∏µ‡πà‡∏´‡∏ô‡∏≤ ‡∏°‡∏µ‡∏Ç‡∏ô‡∏≤‡∏î‡πÉ‡∏´‡∏ç‡πà‡∏°‡∏≤‡∏Å‡∏õ‡∏Å‡∏Ñ‡∏•‡∏∏‡∏°‡∏û‡∏∑‡πâ‡∏ô‡∏ó‡∏µ‡πà‡∏Ñ‡∏£‡∏≠‡∏ö‡∏Ñ‡∏•‡∏∏‡∏°‡∏ó‡∏±‡πà‡∏ß‡∏à‡∏±‡∏á‡∏´‡∏ß‡∏±‡∏î ‡∏Å‡πà‡∏≠‡∏ï‡∏±‡∏ß‡∏™‡∏π‡∏á‡∏°‡∏≤‡∏Å ‡∏ö‡∏≤‡∏á‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏¢‡∏≠‡∏î‡πÄ‡∏°‡∏Ü‡∏à‡∏∞‡πÅ‡∏ú‡πà‡∏≠‡∏≠‡∏Å‡∏Ñ‡∏•‡πâ‡∏≤‡∏¢‡∏£‡∏π‡∏õ‡∏ó‡∏±‡πà‡∏á ‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÄ‡∏Å‡∏¥‡∏î‡∏ù‡∏ô‡∏ï‡∏Å‡∏´‡∏ô‡∏±‡∏Å ‡∏•‡∏°‡πÅ‡∏£‡∏á ‡∏ü‡πâ‡∏≤‡πÅ‡∏•‡∏ö ‡∏ü‡πâ‡∏≤‡∏£‡πâ‡∏≠‡∏á ‡πÄ‡∏Å‡∏¥‡∏î‡∏û‡∏≤‡∏¢‡∏∏‡∏ü‡πâ‡∏≤‡∏Ñ‡∏∞‡∏ô‡∏≠‡∏á ‡∏ö‡∏≤‡∏á‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏°‡∏µ‡∏•‡∏π‡∏Å‡πÄ‡∏´‡πá‡∏ö‡∏ï‡∏Å ‡∏à‡∏∂‡∏á‡∏°‡∏±‡∏Å‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡∏ß‡πà‡∏≤ ‡πÄ‡∏°‡∏Ü‡∏ü‡πâ‡∏≤‡∏Ñ‡∏∞‡∏ô‡∏≠‡∏á')

     elif c_name == c_type[7]:
         st.title(f"**{c_name}** \n")
         st.header('**‡πÄ‡∏°‡∏Ü‡∏Ñ‡∏¥‡∏ß‡∏°‡∏π‡∏•‡∏±‡∏™(‡πÄ‡∏°‡∏Ü‡∏Å‡πà‡∏≠‡∏ï‡∏±‡∏ß‡πÉ‡∏ô‡πÅ‡∏ô‡∏ß‡∏ï‡∏±‡πâ‡∏á):**')
         st.markdown('**‡πÄ‡∏°‡∏Ü‡∏Å‡πà‡∏≠‡∏ï‡∏±‡∏ß‡πÉ‡∏ô‡πÅ‡∏ô‡∏ß‡∏ï‡∏±‡πâ‡∏á:** ‡πÄ‡∏õ‡πá‡∏ô‡πÄ‡∏°‡∏Ü‡∏ó‡∏µ‡πà‡∏Å‡πà‡∏≠‡∏ï‡∏±‡∏ß‡πÉ‡∏ô‡πÅ‡∏ô‡∏ß‡∏ï‡∏±‡πâ‡∏á‡∏ó‡∏µ‡πà‡∏°‡∏µ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏£‡∏∏‡∏ô‡πÅ‡∏£‡∏á ‡πÅ‡∏•‡∏∞‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡∏∂‡πâ‡∏ô‡πÇ‡∏î‡∏¢‡∏â‡∏±‡∏ö‡∏û‡∏•‡∏±‡∏ô ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏Ç‡∏≠‡∏á‡∏ê‡∏≤‡∏ô‡πÄ‡∏°‡∏Ü‡∏≠‡∏¢‡∏π‡πà‡∏ó‡∏µ‡πà‡∏õ‡∏£‡∏∞‡∏°‡∏≤‡∏ì 500 ‡πÄ‡∏°‡∏ï‡∏£ ‡∏™‡πà‡∏ß‡∏ô‡∏¢‡∏≠‡∏î‡πÄ‡∏°‡∏Ü‡∏°‡∏µ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÑ‡∏°‡πà‡πÅ‡∏ô‡πà‡∏ô‡∏≠‡∏ô ‡∏ö‡∏≤‡∏á‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏≠‡∏≤‡∏à‡∏™‡∏π‡∏á‡∏ñ‡∏∂‡∏á‡∏£‡∏∞‡∏î‡∏±‡∏ö‡πÄ‡∏°‡∏Ü‡∏ä‡∏±‡πâ‡∏ô‡∏™‡∏π‡∏á')
         st.markdown('**‡πÄ‡∏°‡∏Ü‡∏ä‡∏ô‡∏¥‡∏î‡∏ô‡∏µ‡πâ‡∏°‡∏µ 2 ‡∏•‡∏±‡∏Å‡∏©‡∏ì‡∏∞:**')
         st.markdown('‡∏´‡∏≤‡∏Å‡∏≠‡∏¢‡∏π‡πà‡πÄ‡∏õ‡πá‡∏ô‡∏Å‡πâ‡∏≠‡∏ô‡πÄ‡∏î‡∏µ‡πà‡∏¢‡∏ß‡πÜ ‡∏•‡∏±‡∏Å‡∏©‡∏ì‡∏∞‡∏õ‡∏∏‡∏Å‡∏õ‡∏∏‡∏¢‡∏Å‡πà‡∏≠‡∏ï‡πà‡∏≠‡πÉ‡∏ô‡πÅ‡∏ô‡∏ß‡∏ï‡∏±‡πâ‡∏á‡∏™‡∏µ‡∏Ç‡∏≤‡∏ß ‡∏™‡πà‡∏ß‡∏ô‡∏ê‡∏≤‡∏ô‡πÄ‡∏°‡∏Ü‡∏à‡∏∞‡∏°‡∏µ‡∏™‡∏µ‡πÄ‡∏ó‡∏≤ ‡∏°‡∏µ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏´‡∏ô‡∏≤‡∏ó‡∏µ‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏ö‡∏î‡∏ö‡∏±‡∏á‡πÅ‡∏ß‡∏á‡∏à‡∏≤‡∏Å‡∏î‡∏ß‡∏á‡∏≠‡∏≤‡∏ó‡∏¥‡∏ï‡∏¢‡πå‡πÑ‡∏î‡πâ ‡∏´‡∏≤‡∏Å‡πÄ‡∏´‡πá‡∏ô‡∏•‡∏±‡∏Å‡∏©‡∏ì‡∏∞‡∏ô‡∏µ‡πâ‡πÅ‡∏™‡∏î‡∏á‡∏ß‡πà‡∏≤‡∏≠‡∏≤‡∏Å‡∏≤‡∏®‡∏î‡∏µ ')
         st.markdown('‡∏´‡∏≤‡∏Å‡∏õ‡∏£‡∏∞‡∏Å‡∏≠‡∏ö‡∏Å‡∏±‡∏ö‡∏ó‡πâ‡∏≠‡∏á‡∏ü‡πâ‡∏≤‡∏°‡∏µ‡∏™‡∏µ‡∏ü‡πâ‡∏≤‡πÄ‡∏Ç‡πâ‡∏° ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏à‡∏±‡∏ö‡∏ï‡∏±‡∏ß‡πÄ‡∏õ‡πá‡∏ô‡∏Å‡∏•‡∏∏‡πà‡∏°‡∏Å‡πâ‡∏≠‡∏ô‡πÉ‡∏´‡∏ç‡πà‡∏Ç‡∏∂‡πâ‡∏ô‡∏°‡∏≤ ‡∏à‡∏∞‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÄ‡∏Å‡∏¥‡∏î‡∏ù‡∏ô‡∏ü‡πâ‡∏≤‡∏Ñ‡∏∞‡∏ô‡∏≠‡∏á‡∏Ç‡∏∂‡πâ‡∏ô‡πÑ‡∏î‡πâ ‡∏ã‡∏∂‡πà‡∏á‡∏≠‡∏≤‡∏à‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÄ‡∏Å‡∏¥‡∏î‡∏õ‡∏£‡∏≤‡∏Å‡∏è‡∏Å‡∏≤‡∏£‡∏ì‡πå‡∏ó‡∏£‡∏á‡∏Å‡∏•‡∏î‡πÅ‡∏•‡∏∞‡∏£‡∏∏‡πâ‡∏á‡πÑ‡∏î‡πâ')

     elif c_name == c_type[8]:
         st.title(f"**{c_name}** \n")
         st.header('**‡∏ô‡∏¥‡∏°‡πÇ‡∏ö‡∏™‡πÄ‡∏ï‡∏£‡∏ï‡∏±‡∏™(‡πÄ‡∏°‡∏Ü‡∏ä‡∏±‡πâ‡∏ô‡∏ï‡πà‡∏≥):**')
         st.markdown('**‡πÄ‡∏°‡∏Ü‡∏ä‡∏±‡πâ‡∏ô‡∏ï‡πà‡∏≥:** ‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡∏∂‡πâ‡∏ô‡∏ó‡∏µ‡πà‡∏£‡∏∞‡∏î‡∏±‡∏ö‡∏ï‡πà‡∏≥‡∏Å‡∏ß‡πà‡∏≤ 2 ‡∏Å‡∏¥‡πÇ‡∏•‡πÄ‡∏°‡∏ï‡∏£ ‡∏õ‡∏£‡∏∞‡∏Å‡∏≠‡∏ö‡∏î‡πâ‡∏ß‡∏¢‡∏≠‡∏ô‡∏∏‡∏†‡∏≤‡∏Ñ‡∏ô‡πâ‡∏≥‡πÄ‡∏Å‡∏∑‡∏≠‡∏ö‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î')
         st.markdown('‡πÄ‡∏°‡∏Ü‡πÅ‡∏ú‡πà‡∏ô‡∏™‡∏µ‡πÄ‡∏ó‡∏≤ ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏™‡∏±‡∏á‡πÄ‡∏Å‡∏ï‡πÄ‡∏´‡πá‡∏ô‡πÄ‡∏°‡∏Ü‡∏ä‡∏ô‡∏¥‡∏î‡∏ô‡∏µ‡πâ‡∏°‡∏±‡∏Å‡∏à‡∏∞‡πÄ‡∏Å‡∏¥‡∏î‡∏ù‡∏ô‡∏û‡∏£‡∏≥‡∏£‡∏∞‡∏¢‡∏∞‡πÄ‡∏ß‡∏•‡∏≤‡∏£‡∏≤‡∏ß 2-3 ‡∏ä‡∏±‡πà‡∏ß‡πÇ‡∏°‡∏á ‡∏ù‡∏ô‡∏ï‡∏Å‡πÅ‡∏ï‡πà‡πÅ‡∏î‡∏î‡∏≠‡∏≠‡∏Å‡∏´‡∏£‡∏∑‡∏≠‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÄ‡∏´‡πá‡∏ô‡∏™‡∏≤‡∏¢‡∏ù‡∏ô‡∏ó‡∏µ‡πà‡∏ï‡∏Å‡∏•‡∏á‡∏°‡∏≤‡∏à‡∏≤‡∏Å‡∏ê‡∏≤‡∏ô‡πÄ‡∏°‡∏Ü‡πÅ‡∏ï‡πà‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏û‡∏≤‡∏¢‡∏∏‡∏ù‡∏ô‡∏ü‡πâ‡∏≤‡∏Ñ‡∏∞‡∏ô‡∏≠‡∏á ‡∏•‡∏±‡∏Å‡∏©‡∏ì‡∏∞‡∏à‡∏∞‡πÄ‡∏õ‡πá‡∏ô‡πÄ‡∏°‡∏Ü‡∏™‡∏µ‡πÄ‡∏ó‡∏≤‡∏ã‡∏∂‡πà‡∏á‡∏à‡∏∞‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡∏ó‡πâ‡∏≠‡∏á‡∏ü‡πâ‡∏≤‡∏î‡∏π‡∏™‡∏•‡∏±‡∏ß‡πÄ‡∏ô‡∏∑‡πà‡∏≠‡∏á‡∏à‡∏≤‡∏Å‡∏°‡∏µ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏´‡∏ô‡∏≤‡∏ó‡∏µ‡πà‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡∏ö‡∏±‡∏á‡∏î‡∏ß‡∏á‡∏≠‡∏≤‡∏ó‡∏¥‡∏ï‡∏¢‡πå‡πÑ‡∏î‡πâ')

     elif c_name == c_type[9]:
         st.title(f"**{c_name}** \n")
         st.header('**‡πÄ‡∏°‡∏Ü‡∏™‡πÄ‡∏ï‡∏£‡πÇ‡∏ï‡∏Ñ‡∏¥‡∏ß‡∏°‡∏π‡∏•‡∏±‡∏™(‡πÄ‡∏°‡∏Ü‡∏ä‡∏±‡πâ‡∏ô‡∏ï‡πà‡∏≥): **')
         st.markdown('**‡πÄ‡∏°‡∏Ü‡∏ä‡∏±‡πâ‡∏ô‡∏ï‡πà‡∏≥:** ‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡∏∂‡πâ‡∏ô‡∏ó‡∏µ‡πà‡∏£‡∏∞‡∏î‡∏±‡∏ö‡∏ï‡πà‡∏≥‡∏Å‡∏ß‡πà‡∏≤ 2 ‡∏Å‡∏¥‡πÇ‡∏•‡πÄ‡∏°‡∏ï‡∏£ ‡∏õ‡∏£‡∏∞‡∏Å‡∏≠‡∏ö‡∏î‡πâ‡∏ß‡∏¢‡∏≠‡∏ô‡∏∏‡∏†‡∏≤‡∏Ñ‡∏ô‡πâ‡∏≥‡πÄ‡∏Å‡∏∑‡∏≠‡∏ö‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î')
         st.markdown('‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÄ‡∏Å‡∏¥‡∏î‡∏ù‡∏ô‡∏ï‡∏Å‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢ ‡∏°‡∏±‡∏Å‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡∏∂‡πâ‡∏ô‡πÉ‡∏ô‡πÄ‡∏ß‡∏•‡∏≤‡∏ó‡∏µ‡πà‡∏≠‡∏≤‡∏Å‡∏≤‡∏®‡πÑ‡∏°‡πà‡∏î‡∏µ ‡∏•‡∏±‡∏Å‡∏©‡∏ì‡∏∞‡πÄ‡∏õ‡πá‡∏ô‡∏Å‡πâ‡∏≠‡∏ô‡∏•‡∏≠‡∏¢‡∏ï‡∏¥‡∏î‡∏Å‡∏±‡∏ô‡πÄ‡∏õ‡πá‡∏ô‡πÅ‡∏û ‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏£‡∏π‡∏õ‡∏ó‡∏£‡∏á‡∏ó‡∏µ‡πà‡∏ä‡∏±‡∏î‡πÄ‡∏à‡∏ô ‡∏°‡∏µ‡∏ä‡πà‡∏≠‡∏á‡∏ß‡πà‡∏≤‡∏á‡∏£‡∏∞‡∏´‡∏ß‡πà‡∏≤‡∏á‡∏Å‡πâ‡∏≠‡∏ô‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢ ‡∏™‡∏µ‡πÄ‡∏ó‡∏≤‡∏´‡∏£‡∏∑‡∏≠‡∏Ñ‡πà‡∏≠‡∏ô‡∏Ç‡πâ‡∏≤‡∏á‡∏Ç‡∏≤‡∏ß ‡πÅ‡∏ï‡πà‡∏Å‡πá‡∏°‡∏µ‡∏™‡πà‡∏ß‡∏ô‡∏ó‡∏µ‡πà‡∏°‡∏∑‡∏î‡∏Ñ‡∏£‡∏∂‡πâ‡∏°‡∏≠‡∏¢‡∏π‡πà‡∏î‡πâ‡∏ß‡∏¢ ‡πÅ‡∏•‡∏∞‡∏≠‡∏≤‡∏à‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÄ‡∏´‡πá‡∏ô‡∏õ‡∏£‡∏≤‡∏Å‡∏è‡∏Å‡∏≤‡∏£‡∏ì‡πå‡∏î‡∏ß‡∏á‡∏≠‡∏≤‡∏ó‡∏¥‡∏ï‡∏¢‡πå‡∏´‡∏£‡∏∑‡∏≠‡∏î‡∏ß‡∏á‡∏à‡∏±‡∏ô‡∏ó‡∏£‡πå‡∏ó‡∏£‡∏á‡∏Å‡∏•‡∏î‡πÑ‡∏î‡πâ‡πÄ‡∏ä‡πà‡∏ô‡∏Å‡∏±‡∏ô ‡πÅ‡∏•‡∏∞‡∏õ‡∏£‡∏≤‡∏Å‡∏è‡∏Å‡∏≤‡∏£‡∏ì‡πå‡πÅ‡∏ñ‡∏ö‡∏™‡∏µ ‡∏´‡∏£‡∏∑‡∏≠‡∏£‡∏∏‡πâ‡∏á')

     elif c_name == c_type[10]:
         st.title(f"**{c_name}** \n ")
         st.header('**‡πÄ‡∏°‡∏Ü‡∏™‡πÄ‡∏ï‡∏£‡∏ï‡∏±‡∏™ ‡∏´‡∏£‡∏∑‡∏≠‡∏™‡∏ï‡∏£‡∏≤‡∏ï‡∏±‡∏™(‡πÄ‡∏°‡∏Ü‡∏ä‡∏±‡πâ‡∏ô‡∏ï‡πà‡∏≥): **')
         st.markdown('**‡πÄ‡∏°‡∏Ü‡∏ä‡∏±‡πâ‡∏ô‡∏ï‡πà‡∏≥:** ‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡∏∂‡πâ‡∏ô‡∏ó‡∏µ‡πà‡∏£‡∏∞‡∏î‡∏±‡∏ö‡∏ï‡πà‡∏≥‡∏Å‡∏ß‡πà‡∏≤ 2 ‡∏Å‡∏¥‡πÇ‡∏•‡πÄ‡∏°‡∏ï‡∏£ ‡∏õ‡∏£‡∏∞‡∏Å‡∏≠‡∏ö‡∏î‡πâ‡∏ß‡∏¢‡∏≠‡∏ô‡∏∏‡∏†‡∏≤‡∏Ñ‡∏ô‡πâ‡∏≥‡πÄ‡∏Å‡∏∑‡∏≠‡∏ö‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î')
         st.markdown('‡πÄ‡∏°‡∏Ü‡∏ä‡∏ô‡∏¥‡∏î‡∏ô‡∏µ‡πâ‡∏°‡∏±‡∏Å‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡∏∂‡πâ‡∏ô‡∏ä‡πà‡∏ß‡∏á‡πÄ‡∏ä‡πâ‡∏≤ ‡∏´‡∏£‡∏∑‡∏≠‡∏´‡∏•‡∏±‡∏á‡∏ù‡∏ô‡∏ï‡∏Å ‡πÅ‡∏•‡∏∞‡∏≠‡∏≤‡∏à‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÄ‡∏Å‡∏¥‡∏î‡∏ù‡∏ô‡∏•‡∏∞‡∏≠‡∏≠‡∏á‡∏Ç‡∏∂‡πâ‡∏ô‡πÑ‡∏î‡πâ ‡∏•‡∏±‡∏Å‡∏©‡∏ì‡∏∞‡πÄ‡∏õ‡πá‡∏ô‡πÅ‡∏ú‡πà‡∏ô‡∏ö‡∏≤‡∏á ‡∏™‡∏µ‡πÄ‡∏ó‡∏≤ ‡∏•‡∏≠‡∏¢‡πÄ‡∏´‡∏ô‡∏∑‡∏≠‡∏û‡∏∑‡πâ‡∏ô‡πÑ‡∏°‡πà‡∏°‡∏≤‡∏Å‡∏ô‡∏±‡∏Å ‡πÄ‡∏ä‡πà‡∏ô ‡πÄ‡∏°‡∏Ü‡∏õ‡∏Å‡∏Ñ‡∏•‡∏∏‡∏°‡∏¢‡∏≠‡∏î‡πÄ‡∏Ç‡∏≤ ‡πÄ‡∏õ‡πá‡∏ô‡∏ï‡πâ‡∏ô')
         




else:
     st.error(f"Sorry, pls take another image") #result display
